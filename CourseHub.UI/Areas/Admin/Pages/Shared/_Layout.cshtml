@{
    Layout = AdminGlobal.LAYOUT_BASE;
}


<div id="page-top">
    <div id="wrapper">
        @await Html.PartialAsync("_Sidebar")
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                @*@await Html.PartialAsync("SharedPartial/_Topbar")*@
                @RenderBody()
            </div>
        </div>
    </div>
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    @*@await Html.PartialAsync("SharedPartial/_Logout")*@
</div>

@await RenderSectionAsync(name: "Script", required: false)
@await RenderSectionAsync(name: "ValidateScript", required: false)